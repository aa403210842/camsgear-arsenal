{
  "name": "arsenal",
  "productName": "Camsgear Arsenal",
  "version": "1.0.0",
  "description": "Camsgear Arsenal - Stitching tool",
  "main": "src/index.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "package32": "electron-forge package --arch=all",
    "make": "electron-forge make",
    "lint": "eslint src"
  },
  "keywords": [],
  "author": "edwin",
  "license": "MIT",
  "config": {
    "forge": {
      "make_targets": {
